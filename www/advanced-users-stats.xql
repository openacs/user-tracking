<?xml version="1.0"?>

<queryset>

    <fullquery name="select_dotlrn_communities_count">
        <querytext>
            select count(*)
            from dotlrn_communities
        </querytext>
    </fullquery>

    <fullquery name="select_dotlrn_user_count">
        <querytext>
            select count(*)
            from dotlrn_users
        </querytext>
    </fullquery>

</queryset>
